<!DOCTYPE html>
<html>
        <head>
                <title><%= title %></title>
                <%= javascript_include_tag :defaults %>
                <%= javascript_include_tag :jquery %>
                <%= javascript_include_tag :rails %>
                <%= javascript_include_tag "application" %>
                <%= javascript_include_tag :prad %>
                <%= javascript_include_tag "http://ajax.googleapis.com/ajax/libs/jquery/1.6.2/jquery.min.js", "http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.1/jquery-ui.min.js", 'autocomplete-rails.js' %>    
                <!--[if lt IE 9]>
                <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
                <![endif]-->
                <%= stylesheet_link_tag 'blueprint/screen', :media => 'screen' %>
                <%= stylesheet_link_tag 'blueprint/print',  :media => 'print' %>
                <!--[if lt IE 8]><%= stylesheet_link_tag 'blueprint/ie' %><![endif]-->
                 <%= stylesheet_link_tag 'custom', :media => 'screen' %>
                <%= csrf_meta_tags %>

         </head>
         <body>

                 <header>
                 <div class = "container">

                         <%= render 'layouts/header' %>
                 </div>
                 </header>
                 <div class="container">
                         <section class="round">
                         <% if signed_in? %>
                                <%= render 'shared/user_info' %>
                         <% end %>
                         <% flash.each do |key, msg| %>
                         <%= content_tag :div, msg, :class => [key, " message"], :id => "notice_ #{key}" %>
                         <% content_tag :script, :type => "text/javascript" do %>
                                 setTimeout("new Effect.Fade('notice_<%= key %>');", 15000);
                          <% end %>
                          <% end %>
                         <%= yield %>
                         </section>
                 </div>

         </body>
</html>
